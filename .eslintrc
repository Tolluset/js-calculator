{
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "cypress/globals": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": ["prettier", "cypress"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "import/no-unresolved": "off",
        "no-new": "off",
        "no-alert": "off"
    }
}
